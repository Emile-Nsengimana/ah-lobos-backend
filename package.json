{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "test": "npm run create_tables && NODE_ENV=test nyc --reporter=text mocha --require babel-register --require babel-polyfill ./tests/ --exit && npm run drop_tables",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "drop_tables": "NODE_ENV=test sequelize db:migrate:undo:all",
    "create_tables": "NODE_ENV=test sequelize db:migrate",
    "start": "node --exec babel-node ./src/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.1",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "body-parser": "^1.19.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1"
  }
}
